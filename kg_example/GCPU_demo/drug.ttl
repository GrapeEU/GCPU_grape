@prefix exdrug: <http://example.org/drug/> .
@prefix excond: <http://example.org/condition/> .
@prefix excommon: <http://example.org/common/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# --- Classe Substance ---
exdrug:Substance rdf:type owl:Class ;
    rdfs:label "Active Substance" .

# --- Propriétés ---
exdrug:contraindicatedFor rdf:type owl:ObjectProperty ;
    rdfs:label "is contraindicated for" .

exdrug:causesSymptom rdf:type owl:ObjectProperty ;
    rdfs:label "causes symptom" .

exdrug:hasMetabolite rdf:type owl:ObjectProperty ;
    rdfs:label "has metabolite" .

exdrug:hasMechanismOfAction rdf:type owl:ObjectProperty ;
    rdfs:label "mechanism of action" .

exdrug:affectsOrgan rdf:type owl:ObjectProperty ;
    rdfs:label "affects organ" .

exdrug:hasChemicalFormula rdf:type owl:DatatypeProperty ;
    rdfs:label "chemical formula" .

exdrug:hasMolecularWeight rdf:type owl:DatatypeProperty ;
    rdfs:label "molecular weight" .

exdrug:hasHalfLife rdf:type owl:DatatypeProperty ;
    rdfs:label "half-life hours" .

# --- Substance E27B (Metformin-like) ---
exdrug:E27B rdf:type exdrug:Substance ;
    rdfs:label "Substance E27B" ;
    exdrug:hasChemicalFormula "C4H11N5" ;
    exdrug:hasMolecularWeight "129.16"^^xsd:float ;
    exdrug:hasHalfLife "6.2"^^xsd:float ;
    exdrug:contraindicatedFor excond:PostNephrectomyStatus ;
    exdrug:contraindicatedFor excond:SevereRenalImpairment ;
    exdrug:contraindicatedFor excond:AcuteMetabolicAcidosis ;
    exdrug:causesSymptom excommon:StomachDiscomfort ;
    exdrug:causesSymptom excommon:Nausea ;
    exdrug:causesSymptom excommon:Diarrhea ;
    exdrug:hasMetabolite exdrug:E27B_M1 ;
    exdrug:hasMechanismOfAction excommon:DecreaseHepaticGlucoseProduction ;
    exdrug:affectsOrgan excommon:Kidney ;
    exdrug:affectsOrgan excommon:Liver ;
    exdrug:affectsOrgan excommon:Intestine .

exdrug:E27B_M1 rdf:type exdrug:Substance ;
    rdfs:label "E27B Metabolite M1" ;
    exdrug:hasHalfLife "2.5"^^xsd:float ;
    exdrug:affectsOrgan excommon:Kidney .


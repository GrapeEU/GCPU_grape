@prefix excond: <http://example.org/condition/> .
@prefix excommon: <http://example.org/common/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# --- Classes ---
excond:Condition rdf:type owl:Class ;
    rdfs:label "Condition" .

excond:Procedure rdf:type owl:Class ;
    rdfs:label "Procedure" .

excommon:Organ rdf:type owl:Class ;
    rdfs:label "Organ" .

excommon:Symptom rdf:type owl:Class ;
    rdfs:label "Symptom" .

excommon:VitalSign rdf:type owl:Class ;
    rdfs:label "Vital Sign" .

excommon:LabResult rdf:type owl:Class ;
    rdfs:label "Laboratory Result" .

excommon:Allergy rdf:type owl:Class ;
    rdfs:label "Allergy" .

excommon:TherapeuticClass rdf:type owl:Class ;
    rdfs:label "Therapeutic Class" .

excommon:MechanismOfAction rdf:type owl:Class ;
    rdfs:label "Mechanism of Action" .

# --- Propriétés ---
excond:resultsInCondition rdf:type owl:ObjectProperty ;
    rdfs:label "results in condition" .

excond:typicalSymptom rdf:type owl:ObjectProperty ;
    rdfs:label "has typical symptom" .

excond:affectsOrgan rdf:type owl:ObjectProperty ;
    rdfs:label "affects organ" .

excond:hasRiskFactor rdf:type owl:ObjectProperty ;
    rdfs:label "has risk factor" .

excond:hasComorbidity rdf:type owl:ObjectProperty ;
    rdfs:label "common comorbidity" .

excond:standardTreatment rdf:type owl:ObjectProperty ;
    rdfs:label "standard treatment includes" .

excommon:semanticallySimilarTo rdf:type owl:ObjectProperty ;
    rdfs:label "is similar to" .

# --- Procédures ---
excond:Nephrectomy2005 rdf:type excond:Procedure ;
    rdfs:label "Nephrectomy (2005)" ;
    excond:resultsInCondition excond:PostNephrectomyStatus .

excond:Nephrectomy2018 rdf:type excond:Procedure ;
    rdfs:label "Nephrectomy (2018)" ;
    excond:resultsInCondition excond:PostNephrectomyStatus .

excond:AppendectomyProcedure rdf:type excond:Procedure ;
    rdfs:label "Appendectomy" ;
    excond:resultsInCondition excond:PostSurgicalStatus .

excond:CataractSurgery rdf:type excond:Procedure ;
    rdfs:label "Cataract Surgery" .

# --- Conditions Principales ---
excond:DiabetesMellitus rdf:type excond:Condition ;
    rdfs:label "Diabetes Mellitus" ;
    excond:typicalSymptom excommon:Polyuria ;
    excond:typicalSymptom excommon:Polydipsia ;
    excond:typicalSymptom excommon:WeightLoss ;
    excond:affectsOrgan excommon:Pancreas ;
    excond:affectsOrgan excommon:Kidney ;
    excond:affectsOrgan excommon:BloodVessels ;
    excond:hasRiskFactor excommon:Obesity ;
    excond:hasRiskFactor excommon:SedentaryLifestyle ;
    excond:hasComorbidity excond:Hypertension ;
    excond:hasComorbidity excond:Hyperlipidemia ;
    excond:hasComorbidity excond:CardiovascularDisease .

excond:PostNephrectomyStatus rdf:type excond:Condition ;
    rdfs:label "Post-Nephrectomy Status" ;
    excond:typicalSymptom excommon:AbdominalPain ;
    excond:typicalSymptom excommon:BackPain ;
    excond:typicalSymptom excommon:Fatigue ;
    excond:affectsOrgan excommon:Kidney ;
    excond:hasComorbidity excond:ChronicKidneyDisease ;
    excond:hasComorbidity excond:Hypertension .

excond:ChronicKidneyDisease rdf:type excond:Condition ;
    rdfs:label "Chronic Kidney Disease" ;
    excond:typicalSymptom excommon:Fatigue ;
    excond:typicalSymptom excommon:Nausea ;
    excond:typicalSymptom excommon:LossOfAppetite ;
    excond:affectsOrgan excommon:Kidney ;
    excond:hasRiskFactor excond:DiabetesMellitus ;
    excond:hasRiskFactor excond:Hypertension ;
    excond:hasComorbidity excond:Anemia ;
    excond:hasComorbidity excond:Hypertension .

excond:Hypertension rdf:type excond:Condition ;
    rdfs:label "Hypertension" ;
    excond:typicalSymptom excommon:Headache ;
    excond:typicalSymptom excommon:Dizziness ;
    excond:affectsOrgan excommon:Heart ;
    excond:affectsOrgan excommon:BloodVessels ;
    excond:affectsOrgan excommon:Kidney ;
    excond:hasRiskFactor excommon:Obesity ;
    excond:hasRiskFactor excommon:HighSaltIntake ;
    excond:hasComorbidity excond:DiabetesMellitus ;
    excond:hasComorbidity excond:Hyperlipidemia .

excond:Hyperlipidemia rdf:type excond:Condition ;
    rdfs:label "Hyperlipidemia" ;
    excond:affectsOrgan excommon:BloodVessels ;
    excond:affectsOrgan excommon:Liver ;
    excond:hasRiskFactor excommon:Obesity ;
    excond:hasRiskFactor excommon:HighFatDiet ;
    excond:hasComorbidity excond:CardiovascularDisease .

excond:CardiovascularDisease rdf:type excond:Condition ;
    rdfs:label "Cardiovascular Disease" ;
    excond:typicalSymptom excommon:ChestPain ;
    excond:typicalSymptom excommon:Dyspnea ;
    excond:affectsOrgan excommon:Heart ;
    excond:affectsOrgan excommon:BloodVessels .

excond:Osteoporosis rdf:type excond:Condition ;
    rdfs:label "Osteoporosis" ;
    excond:typicalSymptom excommon:BonePain ;
    excond:typicalSymptom excommon:Fractures ;
    excond:affectsOrgan excommon:Bone ;
    excond:hasRiskFactor excommon:PostmenopauseStatus .

excond:Anemia rdf:type excond:Condition ;
    rdfs:label "Anemia" ;
    excond:typicalSymptom excommon:Fatigue ;
    excond:typicalSymptom excommon:Pallor ;
    excond:affectsOrgan excommon:Blood ;
    excond:hasComorbidity excond:ChronicKidneyDisease .

excond:HeartFailure rdf:type excond:Condition ;
    rdfs:label "Heart Failure" ;
    excond:typicalSymptom excommon:Dyspnea ;
    excond:typicalSymptom excommon:Edema ;
    excond:affectsOrgan excommon:Heart .

excond:Angina rdf:type excond:Condition ;
    rdfs:label "Angina" ;
    excond:typicalSymptom excommon:ChestPain ;
    excond:affectsOrgan excommon:Heart .

excond:AtrialFibrillation rdf:type excond:Condition ;
    rdfs:label "Atrial Fibrillation" ;
    excond:typicalSymptom excommon:Palpitations ;
    excond:affectsOrgan excommon:Heart .

excond:DeepVeinThrombosis rdf:type excond:Condition ;
    rdfs:label "Deep Vein Thrombosis" ;
    excond:typicalSymptom excommon:LegSwelling ;
    excond:affectsOrgan excommon:BloodVessels .

excond:Pregnancy rdf:type excond:Condition ;
    rdfs:label "Pregnancy" .

excond:ActiveLiverDisease rdf:type excond:Condition ;
    rdfs:label "Active Liver Disease" ;
    excond:affectsOrgan excommon:Liver .

excond:ActiveBleeding rdf:type excond:Condition ;
    rdfs:label "Active Bleeding" .

excond:SevereHypertension rdf:type excond:Condition ;
    rdfs:label "Severe Hypertension" ;
    excond:affectsOrgan excommon:Heart ;
    excond:affectsOrgan excommon:BloodVessels .

excond:SevereRenalImpairment rdf:type excond:Condition ;
    rdfs:label "Severe Renal Impairment" ;
    excond:affectsOrgan excommon:Kidney .

excond:AcuteMetabolicAcidosis rdf:type excond:Condition ;
    rdfs:label "Acute Metabolic Acidosis" .

excond:BilateralRenalArteryStenosis rdf:type excond:Condition ;
    rdfs:label "Bilateral Renal Artery Stenosis" ;
    excond:affectsOrgan excommon:Kidney .

excond:PostSurgicalStatus rdf:type excond:Condition ;
    rdfs:label "Post-Surgical Status" .

